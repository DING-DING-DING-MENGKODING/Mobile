<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/navigation_home"
    android:id="@+id/mobile_navigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.sadarapp.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/navigation_log"
        android:name="com.example.sadarapp.ui.log.LogFragment"
        android:label="Log"
        tools:layout="@layout/fragment_log"/>

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.sadarapp.ui.user.UserFragment"
        android:label="User"
        tools:layout="@layout/fragment_user"/>

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.sadarapp.ui.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings"/>



</navigation>